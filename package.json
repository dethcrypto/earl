{
  "name": "earl-monorepo",
  "private": true,
  "engines": {
    "node": ">=10",
    "yarn": "^1.17.3"
  },
  "workspaces": {
    "packages": ["packages/*"]
  },
  "scripts": {
    "lint": "wsrun -mc lint",
    "format": "wsrun -mc format",
    "typecheck": "wsrun -mc typecheck",
    "build": "wsrun -emc build",
    "build:noweb": "wsrun -x website -emc build",
    "test": "wsrun -emc test"
  },
  "dependencies": {
    "wsrun": "^5.2.1"
  }
}